{
  "name": "test-grammar",
  "version": "1.0.2",
  "description": "utility for testing vscode grammar files",
  "main": "dist/index.js",
  "scripts": {
    "prepack": "tsc -b && npm run generate",
    "test": "nodemon src/test.ts",
    "generate": "npm run generate:docs && npm run generate:badges",
    "generate:badges": "get-badges",
    "generate:docs": "tdg --exclude 'index,logger'"
  },
  "files": [
    "dist"
  ],
  "author": "Syler",
  "license": "MIT",
  "devDependencies": {
    "get-badges": "^0.1.1",
    "nodemon": "^1.19.4",
    "ts-doc-gen": "^0.1.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@sorg/log": "^1.0.3",
    "@types/node": "^12.12.9",
    "vscode-textmate": "^4.3.0"
  }
}
